- var FAQ_MULTI = LANG.products.multi.faq;

include ../../mixins/question

section.faq
	.container
		h2.faq__title.title.title--med!= FAQ_MULTI.title
		.faq__list.js-accordeon
			each question in FAQ_MULTI.questionArray
				+question(question)
		.faq__info 
			p.faq__text.text--med!= FAQ_MULTI.desc
			a.faq__link-email.text--med(href="mailto:support@goodlabs.com") support@goodlabs.com