- var TECHNICAL_MULTI = LANG.products.multi.technical;

include ../../mixins/technical-table

section.technical-info
	.container
		.technical-info__wrapper.technical-info__wrapper--immunal
			.technical-info__left
				h2.technical-info__title.title.title--med!= TECHNICAL_MULTI.title
				p.technical-info__ttl.text--big!= TECHNICAL_MULTI.ttl
				.technical-info__desc
					p.technical-info__text.text--med!= TECHNICAL_MULTI.text_1
					p.technical-info__text.text--med!= TECHNICAL_MULTI.text_2
					p.technical-info__text.text--med!= TECHNICAL_MULTI.text_3
			.technical-info__right
				+technical-table(TECHNICAL_MULTI.technicalInfoArray)